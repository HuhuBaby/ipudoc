{
    "title": "网格通APP",
    "author": "网格通APP",
    "description": "网格通APP 使用说明",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "styles": {
        "website": "./styles/website.css"
    },
    "structure": {
        "readme": "README.md"
    },
    "plugins": [	   
        "-sharing",
        "splitter",
		"chapter-fold",
        "anchors",		
		"github",        
        "sharing-plus",
        "anchor-navigation-ex",
        "favicon",
		"tbfed-pagefooter",
		"hide-element",
		"insert-logo",
		"copy-code-button",
		"timeline",
		"popup",
		"-lunr", 
		"-search", 
		"search-pro"
    ],
	
    "pluginsConfig": {		
        "github": {
            "url": "http://10.1.235.20:3000/ipu/blog.git"
        },        
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": false,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": false,
            "messenger": false,
            "pocket": false,
            "qq": false,
            "qzone": false,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": false,
            "whatsapp": false,
            "all": [
                "qq", "qzone", "weibo"
            ]
        },
        "anchor-navigation-ex": {
            "showLevel": false
        },
        "favicon":{
            "shortcut": "../source/images/favicon.png",
            "bookmark": "../source/images/favicon.png",
            "appleTouch": "../source/images/favicon.png",
            "appleTouchMore": {
                "120x120": "../source/images/favicon.png",
                "180x180": "../source/images/favicon.png"
            }
        },
		"tbfed-pagefooter": {
            "copyright":"Copyright &copy aiipu.com 2017",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
		"hide-element": {
            "elements": [".gitbook-link"]
        },
		"insert-logo": {
			"url": "/doc/source/images/favicon.png",
			"style": "background: none; max-height: 60px; min-height: 30px"
		}
    }
}